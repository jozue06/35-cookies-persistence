    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "jest": "^23.1.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.1.3",
    "morgan": "^1.9.0",
    "require-dir": "^1.0.0",
    "superagent": "^3.8.3",
